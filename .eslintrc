{
  "parser": "@babel/eslint-parser",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "parserOptions": {
    "requireConfigFile": false
  },
  "rules": {
    "object-curly-spacing": "off",
    "require-jsdoc": "off",
    "max-len": "off",
    "no-debugger": "off",
    "no-unused-vars": "off",
    "indent": "off"
  },
  "extends": ["eslint:recommended", "google"]
}
